{
    "builds": [
        {
            "src": "web_app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "web_app/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/web_app/main.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/web_app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/web_app/main.py"
        }
    ]
}